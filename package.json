{
  "name": "nicest",
  "version": "0.7.0",
  "description": "nicely integrating complex education software together",
  "main": "lib/server.js",
  "bin": {
    "nicest": "bin/cli.js"
  },
  "scripts": {
    "postinstall": "jsdoc modules/*/*/*.js -d docs",
    "prestart": "jsdoc modules/*/*/*.js -d docs",
    "pretest": "eslint .",
    "start": "node bin/cli",
    "test": "lab modules/*/*/test.js -m 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristianMurphy/nicest.git"
  },
  "keywords": [
    "education"
  ],
  "author": "Christian Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChristianMurphy/nicest/issues"
  },
  "homepage": "https://github.com/ChristianMurphy/nicest#readme",
  "dependencies": {
    "boom": "2.8.0",
    "hapi": "8.8.0",
    "jade": "1.11.0",
    "joi": "6.5.0",
    "jsdoc": "3.3.2",
    "lodash": "3.10.0",
    "lout": "6.2.3",
    "mongoose": "4.0.7",
    "yargs": "3.15.0"
  },
  "devDependencies": {
    "code": "1.4.1",
    "eslint": "1.0.0-rc-1",
    "lab": "5.13.0"
  },
  "engines": {
    "node": ">=2.0.0"
  }
