extends ../../shared-template/default.pug

block title
    title Code Project Management 

block body
    .ui.container
        h1.ui.center.aligned.header Manage Code Project
        .ui.segment
            .ui.vertical.segment
                input.ui.fluid.large.teal.submit.button(type='submit', value='add student to existing code project')
            .ui.vertical.segment
                input.ui.fluid.large.teal.submit.button(type='submit', value='remove student from existing code project')
            .ui.vertical.segment
                input.ui.fluid.large.teal.submit.button(type='submit', value='transfer student to existing code project')
